# API Configuration
API_URL = "http://localhost:8000"

# Logging Configuration
LOG_FORMAT=%(asctime)s - [%(user_id)s] - [%(session_id)s] - %(name)s - %(levelname)s - %(message)s

# Development
LOG_LEVEL=DEBUG
LOG_TO_FILE=true

# Docker: run tests on container startup (true = run and exit if fail, false = skip tests)
RUN_TESTS=true







